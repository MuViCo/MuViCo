{
  "name": "muvico",
  "private": true,
  "version": "1.0.0",
  "main": "src/index.js",
  "scripts": {
    "start": "docker compose up",
    "dev": "NODE_ENV=development concurrently \"nodemon --exec node src/server/index.js\" \"vite --host\"",
    "prod": "node src/server/index.js",
    "lint": "eslint 'src/**/*.js' 'src/**/*.jsx'",
    "format": "prettier --write '*.{js,json,css,md}'",
    "test-frontend": "jest --testPathPattern=src/client/tests/unit --verbose --runInBand --env=jsdom",
    "build": "vite build",
    "test-backend": "NODE_ENV=test jest --testPathPattern=src/server/tests/ --verbose --runInBand --env=node",
    "test:cypress-open": "cypress open",
    "test-all": "npm run test-frontend && npm run test-backend",
    "test-coverage": "npm run test-frontend && codecov && npm run test-backend && codecov",
    "setup-e2e": "concurrently \"vite --host\"  \"NODE_ENV=test node src/server/index.js\"",
    "e2e": "concurrently \"npm run setup-e2e\"  \"sleep 1 && playwright test --ui\"",
    "e2e:report": "playwright show-report"
  },
  "jest": {
    "globalTeardown": "./src/server/tests/teardown.js",
    "collectCoverage": true,
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/cypress/",
      "/dist/",
      "/coverage/",
      "/public/",
      ".*/src/client/tests/e2e/",
      ".*/src/server/tests/test_helper.js",
      ".*/src/server/connections/",
      ".*/src/server/routes/connections.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MuViCo/MuViCo"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.525.0",
    "@aws-sdk/s3-request-presigner": "^3.529.1",
    "@chakra-ui/icons": "^2.1.1",
    "@chakra-ui/react": "^2.8.2",
    "@chakra-ui/theme-tools": "^2.1.2",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.6",
    "@mui/material": "^5.14.6",
    "@vitejs/plugin-react": "^4.0.4",
    "axios": "^1.6.5",
    "bcrypt": "^5.1.1",
    "bootstrap": "^5.3.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "formik": "^2.4.5",
    "framer-motion": "^11.0.3",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.3.0",
    "mongoose": "^8.0.0",
    "mongoose-unique-validator": "^5.0.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "net": "^1.0.2",
    "notistack": "^3.0.1",
    "npm": "^10.2.4",
    "openid-client": "^5.4.3",
    "passport": "^0.6.0",
    "playwright": "^1.43.0",
    "react": "^18.2.0",
    "react-bootstrap": "^2.10.0",
    "react-dom": "^18.2.0",
    "react-flow-renderer": "^10.3.17",
    "react-new-window": "^1.0.1",
    "react-player": "^2.14.1",
    "react-router-dom": "^6.21.3",
    "reactflow": "^11.10.4",
    "screenfull": "^6.0.2",
    "vite": "^4.4.9",
    "yup": "^1.3.3"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.22.11",
    "@babel/preset-env": "^7.23.9",
    "@babel/preset-react": "^7.23.3",
    "@esbuild/linux-x64": "^0.20.0",
    "@playwright/test": "^1.43.0",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^20.12.5",
    "aws-sdk-mock": "^5.9.0",
    "codecov": "^3.8.3",
    "concurrently": "^8.2.1",
    "cypress": "^12.17.4",
    "eslint": "^8.47.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-cypress": "^2.14.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^14.0.1",
    "nodemon": "^3.0.3",
    "prettier": "^3.0.2",
    "supertest": "^6.3.4"
  }
}
