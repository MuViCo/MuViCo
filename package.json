{
  "name": "muvico",
  "private": true,
  "version": "1.0.0",
  "main": "src/index.js",
  "scripts": {
    "start": "docker-compose up",
    "dev": "NODE_ENV=development concurrently \"nodemon --exec node src/server/index.js\" \"vite --host\"",
    "prod": "NODE_ENV=production node src/server/index.js",
    "lint": "eslint 'src/**/*.js' 'src/**/*.jsx'",
    "format": "prettier --write '*.{js,json,css,md}'",
    "test-frontend": "jest --testPathPattern=src/client/tests/unit --verbose --runInBand --env=jsdom --coverage --coverageReporters=\"html\"",
    "build": "vite build",
    "test-backend": "NODE_ENV=test jest --testPathPattern=src/server/tests/ --verbose --runInBand --env=node",
    "test:cypress-open": "cypress open",
    "test-all": "npm run test-frontend && npm run test-backend",
    "setup-e2e": "concurrently \"vite --host\"  \"NODE_ENV=test node src/server/index.js\"",
    "e2e": "concurrently \"npm run setup-e2e\"  \"sleep 1 && playwright test --project chromium --ui\"",
    "e2e:report": "playwright show-report && codecov"
  },
  "jest": {
    "globalTeardown": "./src/server/tests/teardown.js",
    "collectCoverage": true,
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/cypress/",
      "/dist/",
      "/coverage/",
      "/public/",
      ".*/src/client/tests/e2e/",
      ".*/src/server/tests/test_helper.js",
      ".*/src/server/connections/",
      ".*/src/server/routes/connections.js"
    ],
    "moduleNameMapper": {
      "\\.(css|less|scss|sass)$": "<rootDir>/__mocks__/styleMock.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MuViCo/MuViCo"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.654.0",
    "@aws-sdk/s3-request-presigner": "^3.654.0",
    "@chakra-ui/icons": "^2.1.1",
    "@chakra-ui/react": "^2.8.2",
    "@chakra-ui/theme-tools": "^2.1.2",
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@reduxjs/toolkit": "^2.2.8",
    "@vitejs/plugin-react": "^4.3.1",
    "axios": "^1.6.5",
    "bcrypt": "^5.1.1",
    "bootstrap": "^5.3.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.1",
    "express": "^4.21.0",
    "firebase": "^11.0.1",
    "firebase-admin": "^12.7.0",
    "framer-motion": "^11.5.4",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.9.0",
    "mongoose": "^8.6.3",
    "mongoose-unique-validator": "^5.0.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "notistack": "^3.0.1",
    "npm": "^10.2.4",
    "passport": "^0.7.0",
    "playwright": "^1.47.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-flow-renderer": "^10.3.17",
    "react-grid-layout": "^1.4.4",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.26.2",
    "reactflow": "^11.11.4",
    "redux": "^5.0.1",
    "screenfull": "^6.0.2",
    "vite": "^5.4.6",
    "yup": "^1.3.3"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.25.1",
    "@babel/preset-env": "^7.25.4",
    "@babel/preset-react": "^7.24.7",
    "@playwright/test": "^1.47.1",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^22.5.5",
    "aws-sdk-mock": "^6.2.0",
    "codecov": "^3.8.3",
    "concurrently": "^9.0.1",
    "esbuild": "^0.23.1",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.36.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.12",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.0",
    "nodemon": "^3.1.5",
    "prettier": "^3.3.3",
    "supertest": "^7.0.0"
  }
}
